<h1>Dividend Reporter</h1>
<p>


  <% if @stocks %>
    <% @stocks.each do |st| %>
        <%= link_to st.stock_symbol.upcase, stock_path(st) %> 
      <%= st.stock_name %>
      <%= st.dividend %>
      <%= st.ex_date %>
      <%= st.dividend_per_share %>
      <%= st.last_price %>
      <%= st.collected_at %>
      
      <%= link_to "Watch", watch_stock_path(:stock_id => st.id) %>
</p>
    <% end %>
  <% end %>
<% if current_user.email == "admin@example.com" %>
<%= link_to "Update all quotes", update_stocks_path %>
<%= link_to "New Stock", new_stock_path %>
<% end %>

